<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../partials/head %>
</head>
<body class="container-fluid">

<header>
    <% include ../partials/header %>
</header>

<main>
    <div class="jumbotron">
        <h1><%= h1 %></h1>
        <p><%= p %></p>

				<% if(routes) { %>
        <ul>
				    <% routes.forEach(function(route) { %>
				        <li>
				        	<a target="_blank" href="<%= route.path %>"><%= route.path %></a>
				        	<% route.methods.forEach(function(m) { %>
				        			<span class="label <%= m.c %>"><%= m.name %></span>
				        	<% }); %>
				        </li>
				    <% }); %>
				</ul>
				<% } %>
    </div>
</main>

<footer>
    <% include ../partials/footer %>
</footer>

</body>
</html>
